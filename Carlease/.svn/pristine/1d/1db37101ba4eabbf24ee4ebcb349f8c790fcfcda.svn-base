	public static final int code502 = HTTP_BAD_GATEWAY;
	
	// Numeric status code, 503: Unavailable 
	public static final int code503 = HTTP_UNAVAILABLE;
	
	// Numeric status code, 504: Gateway timeout 
	public static final int code504 = HTTP_GATEWAY_TIMEOUT;
	
