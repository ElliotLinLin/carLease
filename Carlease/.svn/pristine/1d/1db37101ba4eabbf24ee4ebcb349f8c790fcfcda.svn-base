	public static final int code300 = HTTP_MULT_CHOICE;
	
	// Numeric status code, 301 Moved permanently 
	public static final int code301 = HTTP_MOVED_PERM;
	
	// Numeric status code, 302: Moved temporarily 
	public static final int code302 = HTTP_MOVED_TEMP;
	
