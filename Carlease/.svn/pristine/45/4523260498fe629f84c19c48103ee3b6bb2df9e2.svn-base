package com.hst.Carlease.http.bean;

import java.io.Serializable;
import java.util.ArrayList;

public class HotCarBean {

	private int TotalCount;//
	private ArrayList<PageDetails> DataList;// Array

	public int getTotalCount() {
		return TotalCount;
	}

	public void setTotalCount(int totalCount) {
		TotalCount = totalCount;
	}

	public ArrayList<PageDetails> getDataList() {
		return DataList;
	}

	public void setDataList(ArrayList<PageDetails> dataList) {
		DataList = dataList;
	}

	public class PageDetails implements Serializable {
		private static final long serialVersionUID = 1L;
		private ArrayList<String> CarModelUrl;// ; Array
		private String CarModerName;// 宝骏630 1.5MT DVVT

		private String Maddress;// 地址
		private String Mpayment;// 车型首付
		private String hcm_Crt_Date;// 开始时间
		private int hcm_ID;// id

		public ArrayList<String> getCarModelUrl() {
			return CarModelUrl;
		}

		public void setCarModelUrl(ArrayList<String> carModelUrl) {
			CarModelUrl = carModelUrl;
		}

		public String getCarModerName() {
			return CarModerName;
		}

		public void setCarModerName(String carModerName) {
			CarModerName = carModerName;
		}

		public String getMaddress() {
			return Maddress;
		}

		public void setMaddress(String maddress) {
			Maddress = maddress;
		}

		public String getMpayment() {
			return Mpayment;
		}

		public void setMpayment(String mpayment) {
			Mpayment = mpayment;
		}

		public String getHcm_Crt_Date() {
			return hcm_Crt_Date;
		}

		public void setHcm_Crt_Date(String hcm_Crt_Date) {
			this.hcm_Crt_Date = hcm_Crt_Date;
		}

		public int getHcm_ID() {
			return hcm_ID;
		}

		public void setHcm_ID(int hcm_ID) {
			this.hcm_ID = hcm_ID;
		}

	}

}
